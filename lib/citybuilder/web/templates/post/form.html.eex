                </div>
            </div>
        </div>
        <div class="col-sm-4">
            <div class="card">
                <div class="card-block">
                    <div class="text-center">
                        <%= textarea f, :done_so_far_two_title, rows: "4",cols: "10", class: "form-control form-progress-title form-control-progress-title-two", placeholder: "Step Two Title" %>
                            <%= error_tag f, :done_so_far_two_title %>
                                <%= textarea f, :done_so_far_two, rows: "10",cols: "40", class: "form-control form-progress-text form-control-progress-text-one", placeholder: "What have you done so far? Part Two" %>
                                    <%= error_tag f, :done_so_far_two %>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-sm-4">
            <div class="card">
                <div class="card-block">
                    <div class="text-center">
                        <%= textarea f, :done_so_far_three_title, rows: "4",cols: "10", class: "form-control form-progress-title form-control-progress-title-three", placeholder: "Step Three Title" %>
                            <%= error_tag f, :done_so_far_three_title %>
                                <%= textarea f, :done_so_far_three, rows: "10",cols: "40", class: "form-control form-progress-text form-control-progress-text-one", placeholder: "What's the third step you took?" %>
                                    <%= error_tag f, :done_so_far_three %>
                    </div>
                </div>
            </div>
        </div>
    </div>
