<div class="progress-container">
    <div class="row" style="margin:20px">
        <div class="col-sm-4">
            <div class="card">
                   <div class="card-block">
                  
                    <div class="text-center">
                                                    <%= textarea f, :done_so_far_one_title, rows: "4",cols: "10", class: "form-control form-progress-title form-control-progress-title-one", placeholder: "Step One Title" %>
